FRAMEWORK_SEARCH_PATHS = "$(PODS_ROOT)/Realm/realm-cocoapods-0.81.0/ios"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/AFNetworking" "${PODS_ROOT}/Headers/FlatUIKit" "${PODS_ROOT}/Headers/Realm"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/AFNetworking" -isystem "${PODS_ROOT}/Headers/FlatUIKit" -isystem "${PODS_ROOT}/Headers/Realm"
OTHER_LDFLAGS = -ObjC -lc++ -framework CoreGraphics -framework CoreText -framework MobileCoreServices -framework Realm -framework Security -framework SystemConfiguration
PODS_ROOT = ${SRCROOT}/Pods